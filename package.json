    "@supabase/supabase-js": "^2.77.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "graphql": "^16.11.0",
    "motion": "^12.23.24",
    "next": "^15.5.7",
    "next-auth": "^5.0.0-beta.29",
    "prisma": "^6.15.0",
    "react": "19.1.0",
